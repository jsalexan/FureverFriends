      {{!-- Gender --}}
      <div class="profile-group">
        <label id="dog-gender">Gender:</label>
        <div class="form-check">
          <input class="form-check-input gender" type="checkbox" value="Male" id="defaultCheck1">
          <label class="form-check-label" for="defaultCheck1">
            Male
          </label>
        </div>
        <div class="form-check">
          <input class="form-check-input gender" type="checkbox" value="Female" id="defaultCheck1">
          <label class="form-check-label" for="defaultCheck1">
            Female
          </label>
        </div>
      </div>

      {{!-- Size --}}
      <div class="profile-group">
        <label id="dog-size">Size:</label>
        <div class="form-check">
          <input class="form-check-input size" type="checkbox" value="Small" id="defaultCheck1">
          <label class="form-check-label" for="defaultCheck1">
